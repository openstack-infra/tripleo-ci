[DEFAULT]
base={{ ansible_distribution|lower }}
type=binary
registry={{ push_registry }}
tag={{ version_hash }}
template_override={{ openstack_git_root }}/tripleo-common/container-images/tripleo_kolla_template_overrides.j2
